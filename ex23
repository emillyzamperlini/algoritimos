nota = float(input('digite a nota :'))

if nota >= 0 and nota <= 2 :
     print("sem rendimento")
elif nota >= 2 and nota <= 4 :
     print("mau")
elif nota >= 4 and nota <= 6 :
     print("regular")
elif nota >= 6 and nota <= 8.5 :
     print("bom")
elif nota >= 8.5 and nota <= 10 :
     print("excelente")
    